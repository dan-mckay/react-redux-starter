{
  "name": "react-redux-starter",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > public/bundle.js",
    "start": "ecstatic -p 8000 public",
    "watch": "watchify main.js -o public/bundle.js -dv"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.3"
  },
  "devDependencies": {
    "babelify": "^6.4.0",
    "browserify": "^11.2.0",
    "ecstatic": "^1.1.3",
    "react": "^0.14.0",
    "redux-devtools": "^2.1.5",
    "uglify-js": "^2.5.0",
    "watchify": "^3.4.0"
  }
}
