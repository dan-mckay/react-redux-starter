{
  "name": "react-redux-starter",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify **/*.js | uglifyjs -cm > public/bundle.js",
    "start": "ecstatic -p 8000 public",
    "watch": "watchify **/*.js -o public/bundle.js -dv"
  },
  "dependencies": {
    "react": "*",
    "react-redux": "*",
    "redux": "*"
  },
  "devDependencies": {
    "babelify": "*",
    "browserify": "*",
    "ecstatic": "*",
    "react": "*",
    "redux-devtools": "*",
    "uglify-js": "*",
    "watchify": "*"
  }
}
